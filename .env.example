# ADX-Agent Environment Configuration
# Complete example with sample values

# Google Generative AI API Key (Gemini)
# Get your API key from: https://aistudio.google.com/app/apikey
GOOGLE_GENERATIVE_AI_API_KEY=your_google_generative_ai_api_key_here

# E2B API Key
# Get your API key from: https://e2b.dev
E2B_API_KEY=your_e2b_api_key_here

# HuggingFace Token (for advanced AI models)
# Get your token from: https://huggingface.co/settings/tokens
HF_TOKEN=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

# Database Configuration
DB_PASSWORD=secure_password_here
POSTGRES_DB=agent_db
POSTGRES_USER=agent_user

# Redis Configuration
REDIS_PASSWORD=agent_redis_password

# VNC Password (optional, for desktop access)
VNC_PASSWORD=your_vnc_password_here

# Monitoring
GRAFANA_PASSWORD=admin_password_here

# Application Configuration
NODE_ENV=production
NEXT_PUBLIC_API_URL=http://backend:8000

# Development Settings (set to false for production)
DEBUG=true
LOG_LEVEL=info

# Security
JWT_SECRET=your_jwt_secret_here
ENCRYPTION_KEY=your_encryption_key_here

# Rate Limiting
RATE_LIMIT_REQUESTS_PER_MINUTE=100
RATE_LIMIT_BURST_SIZE=20

# Session Configuration
SESSION_TIMEOUT_MINUTES=30
MAX_CONCURRENT_SESSIONS=10